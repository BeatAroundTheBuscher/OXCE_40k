      groups: [3]
      conditionals: [1]
    - type: fillArea
      rects:
        - [1, 0, 1, 5]
      groups: [3]
      conditionals: [2]
    - type: fillArea
      rects:
        - [2, 0, 1, 5]
      groups: [3]
      conditionals: [-2]
    - type: fillArea
      rects:
        - [3, 0, 1, 5]
      groups: [3]
      conditionals: [2]
    - type: fillArea
      rects:
        - [4, 0, 1, 5]
      groups: [3]
      conditionals: [3]
    #*** Place some blocks for assault squad reinforcements ***
    - type: addBlock
      executions: 2
      rects:
        - [0, 4, 5, 1]
      terrain: WAR_TANITH_CONVOYAMBUSH_REINFORCEMENTS
    #*** Fill the 'road' where the convoy is ***
    - type: fillArea
      rects:
        - [1, 0, 1, 5]
      conditionals: [1]
      blocks: [3, 4, 10, 11, 12, 13, 14]
      freqs: [1, 1, 2, 2, 2, 2, 2]
      maxUses: [2, 2, 2, 2, 2, 2, 2]
    - type: fillArea
      rects:
        - [2, 0, 1, 5]
      conditionals: [2]
      blocks: [3, 4, 10, 11, 12, 13, 14]
      freqs: [1, 1, 2, 2, 2, 2, 2]
      maxUses: [2, 2, 2, 2, 2, 2, 2]
    - type: fillArea
      rects:
        - [3, 0, 1, 5]
      conditionals: [3]
      blocks: [3, 4, 10, 11, 12, 13, 14]
      freqs: [1, 1, 2, 2, 2, 2, 2]
      maxUses: [2, 2, 2, 2, 2, 2, 2]
    #*** Place the player's craft ***
    - type: addCraft
      craftName: STR_VALKYRIE_TANITH_CONVOYAMBUSH
    #*** Fill in the rest of the map
    - type: addBlock
      size: 2
      executions: 3
      blocks: [5, 6, 7, 8, 9]
      maxUses: [1, 1, 1, 1, 1]
    - type: fillArea
