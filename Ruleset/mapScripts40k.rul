    - type: addUFO
      UFOName: STR_CHAOS_RHINO_CONVOY
      rects:
        - [1, 0, 3, 4]
    #*** Figure out where it was placed ***
    - type: checkBlock
      rects:
        - [1, 0, 1, 5]
      label: 1
    - type: checkBlock
      rects:
        - [2, 0, 1, 5]
      label: 2
    - type: checkBlock
      rects:
        - [3, 0, 1, 5]
      label: 3
