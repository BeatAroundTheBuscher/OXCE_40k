    - type: digTunnel
      conditionals: [-1]
      direction: both
      tunnelData:
        level: 1
        MCDReplacements:
          - type: westWall
            set: 1
            entry: 13
          - type: northWall
            set: 1
            entry: 14
    - type: fillArea
      conditionals: [-1]
      blocks: 20
